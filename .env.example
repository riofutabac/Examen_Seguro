# Configuración de JWT
JWT_SECRET_KEY=your-super-secure-jwt-secret-key-at-least-32-chars-long

# Configuración de entorno
ENV=development

# Configuración de base de datos (opcional si usas Docker)
POSTGRES_HOST=db
POSTGRES_PORT=5432
