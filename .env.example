DEFAULT_CAJERO_USERNAME=cajero_admin
DEFAULT_CAJERO_PASSWORD=CajeroSecure123!
DEFAULT_CAJERO_EMAIL=cajero@corebank.com
DEFAULT_CAJERO_FULLNAME=Cajero Administrador